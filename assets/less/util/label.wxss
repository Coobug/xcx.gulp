.weui-tag {
  display: inline-block;
  font-size: 13px;
  height: 28px;
  line-height: 28px;
  color: #333;
  border: 1rpx solid #999;
  padding: 0px 10px;
  border-radius: 2px;
  margin-right: 10px;
  box-sizing: border-box;
  vertical-align: middle;
  text-align: center;
}

.weui-tag__red {
  color: #fff;
  background-color: #f44;
  border: 1rpx solid #f44;
}

.weui-tag__green {
  color: #fff;
  background-color: #09bb07;
  border: 1rpx solid #09bb07;
}

.weui-tag__blue {
  color: #fff;
  background-color: #377ade;
  border: 1rpx solid #377ade;
}

.weui-tag__primary {
  color: #fff;
  background-color: @weuiColorPrimary;
  border: 1rpx solid @weuiColorPrimary;
}

.weui-tag__disabled {
  color: #c0c0c0;
  background-color: #f0f0f0;
  border: 1rpx solid #cecece;
}

.weui-tag__small {
  font-size: 11px;
  height: 16px;
  line-height: 16px;
  padding: 0px 3px;
}

.weui-tag__plain {
  &.weui-tag__red {
    color: #f44;
    background-color: #fff;
  }

  &.weui-tag__green {
    color: #09bb07;
    background-color: #fff;
  }

  &.weui-tag__primary {
    color: @weuiColorPrimary;
    background-color: #fff;
  }

  &.weui-tag__blue {
    color: #377ade;
    background-color: #fff;
  }

  &.weui-tag__disabled {
    color: #aaa;
    background-color: #fff;
  }
}

.weui-logo-refresh {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: center 0;
    background-size: 25%;
}